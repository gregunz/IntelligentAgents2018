<?xml version="1.0" encoding="ISO-8859-1"?>

<agents>
    <!-- A list of agents and their behavior classes -->

    <!-- #### AUCTION AGENTS ### -->
    <agent name="auction-random">
        <set class-path="bin/"/>
        <set class-name="agent.AuctionTemplate"/>
    </agent>

    <agent name="auction-default"><!-- # WITH ALL DEFAULT VALUES # -->
        <set class-path="bin/"/>
        <set class-name="agent.AuctionAgent"/>

        <set bidRate="1.0"/>
        <set learningRate="0.1"/>

        <set useMinOfAdvBids="true"/><!-- # NOT NECESSARY (because default is true) # -->

        <set useMinOfAdvBids="true"/><!-- # NOT NECESSARY (because default is true) # -->
        <set numLatestBids="5"/>

        <set useImportance="true"/><!-- # NOT NECESSARY (because default is true) # -->
        <set posWeight="0.3333"/>
        <set probWeight="0.3333"/>
        <set weightWeight="0.3333"/>
    </agent>

    <agent name="auction-agent-01">
        <set class-path="bin/"/>
        <set class-name="agent.AuctionAgent"/>

        <set bidRate="1.0"/>
        <set learningRate="0.1"/>
        <set numLatestBids="5"/>

        <set useImportance="false"/>
    </agent>

</agents>
