<?xml version="1.0" encoding="ISO-8859-1"?>

<agents>
    <!-- A list of agents and their behavior classes -->

    <!-- #### AUCTION AGENTS ### -->
    <agent name="auction-random">
        <set class-path="bin/"/>
        <set class-name="agent.AuctionTemplate"/>
    </agent>

    <agent name="auction-default"><!-- # WITH ALL DEFAULT VALUES # -->
        <set class-path="bin/"/>
        <set class-name="agent.AuctionAgent"/>

        <set verbosity="2"/>

        <set bidRate="1.0"/>
        <set learningRate="0.1"/>

        <set useEarlyBid="true"/>

        <set useMinOfAdvBids="true"/>
        <set numLatestBids="5"/>

        <set useImportance="true"/>
        <set posWeight="0.3333"/>
        <set probWeight="0.3333"/>
        <set weightWeight="0.3333"/>
    </agent>

    <agent name="auction-simplest">
        <set class-path="bin/"/>
        <set class-name="agent.AuctionAgent"/>

        <set bidRate="1.0"/>
        <set learningRate="0.0"/>

        <set useEarlyBid="false"/>
        <set useMinOfAdvBids="false"/>
        <set useImportance="false"/>
    </agent>

</agents>
