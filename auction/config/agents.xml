        <set inLearningRate="0.1"/>
        <set deLearningRate="0.15"/>

        <set smallestBid="1"/>

        <set useEarlyBidStrategy="false"/>

        <set useImportanceStrategy="true"/>
        <set importanceLR="0.1"/>
        <set timeForAdvPlanner="500"/>
        <set posWeight="0.0"/>
        <set probWeight="0.0"/>
        <set weightWeight="1.0"/>
        <set marginalWeight="0.0"/>

        <set useMinOfAdvBidsStrategy="false"/>

