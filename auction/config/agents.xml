<?xml version="1.0" encoding="ISO-8859-1"?>

<agents>
    <!-- A list of agents and their behavior classes -->

    <agent name="default">
        <set class-path="bin/"/>
        <set class-name="agent.AuctionAgent"/>

        <set verbosity="2"/>

        <set bidRate="1.0"/>
        <set inLearningRate="0.05"/>
        <set deLearningRate="0.1"/>

        <set smallestBid="1"/>

        <set useEarlyBidStrategy="true"/>
        <set earlyRate="0.5"/>
        <set numEarlyBids="5"/>

        <set useImportanceStrategy="true"/>
        <set importanceLR="0.2"/>
        <set timeForAdvPlanner="1000"/>
        <set posWeight="0.25"/>
        <set probWeight="0.25"/>
        <set weightWeight="0.25"/>
        <set marginalWeight="0.25"/>

        <set useMinOfAdvBidsStrategy="true"/>
        <set numOfAdvLatestBids="5"/>
        <set numOfOurLatestBids="5"/>
        <set difWithLatestBids="2"/>

    </agent>

    <agent name="simplest">
        <set class-path="bin/"/>
        <set class-name="agent.AuctionAgent"/>

        <set verbosity="2"/>

        <set bidRate="1.0"/>
        <set inLearningRate="0.0"/>
        <set deLearningRate="0.0"/>

        <set smallestBid="1"/>

        <set useEarlyBidStrategy="false"/>
        <set useImportanceStrategy="false"/>
        <set useMinOfAdvBidsStrategy="false"/>
    </agent>


    <agent name="OVSS">
        <set class-path="agents/ovss.jar"/>
        <set class-name="AuctionAgent.AgentTeamWL"/>
    </agent>

</agents>
