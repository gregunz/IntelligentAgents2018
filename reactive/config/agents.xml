<?xml version="1.0" encoding="ISO-8859-1"?>

<agents>
	<!-- A list of agents and their behavior classes -->

    <!-- #### TEMPLATE AGENTS ### -->
    <agent name="template">
		<set class-path="bin/"/>
        <set class-name="agents.TemplateAgent"/>
        <set discount-factor=".95"/>
	</agent>

    <!-- #### DUMMY AGENTS ### -->
    <agent name="dummy">
        <set class-path="bin/"/>
        <set class-name="agents.DummyAgent"/>
        <set discount-factor=".95"/>
    </agent>

    <!-- #### REACTIVE AGENTS ### -->
    <agent name="reactive">
        <set class-path="bin/"/>
        <set class-name="agents.ReactiveAgent"/>
        <set discount-factor=".95"/>
    </agent>
	
</agents>
